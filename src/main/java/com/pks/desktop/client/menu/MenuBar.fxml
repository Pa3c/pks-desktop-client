<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar
	fx:controller="com.pks.desktop.client.menu.MenuBarController"
	minWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/8.0.171">
	<menus>
		<Menu mnemonicParsing="false" text="Otwórz" fx:id="openMenu">
			<items>
			<MenuItem mnemonicParsing="false" text="Panel Szukania" onAction="#openSearchPanel"/>
			</items>
		</Menu>
		<Menu mnemonicParsing="false" text="Opcje" fx:id="optionsMenu">
			<items>
				<MenuItem mnemonicParsing="false" text="Wyloguj się" />
			</items>
		</Menu>
		<Menu mnemonicParsing="false" text="Pomoc" fx:id="helpMenu">
			<items>
				<MenuItem mnemonicParsing="false" text="O Aplikacji !" />
			</items>
		</Menu>
	</menus>
	<padding>
		<Insets left="2.0" top="2.0" />
	</padding>
</MenuBar>
